{
  "name": "foodease-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "migrate:up": "node-pg-migrate up -m ./src/database/migrations -d DATABASE_URL --no-reject-unauthorized",
    "migrate:down": "node-pg-migrate down -m ./src/database/migrations -d DATABASE_URL --no-reject-unauthorized",
    "migrate:redo": "node-pg-migrate redo -m ./src/database/migrations -d DATABASE_URL --no-reject-unauthorized"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^7.4.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.13.0",
    "@types/pg": "^8.11.6",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
